{
  "name": "systray",
  "version": "2.0.0-snapshot",
  "description": "An systray libray for nodejs",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "postinstall": "node ./fetch-prebuilt-helper.js",
    "build": "npm run clean && npm run build:lib",
    "build:lib": "tsc",
    "clean": "del ./lib/**",
    "lint": "tslint ./src/** --type-check -p ./tsconfig.json",
    "test": "cross-env DEBUG=systray* mocha -r ts-node/register ./test/**/*.test.ts",
    "doc": "typedoc --theme minimal --out ./docs ./src && touch ./docs/.nojekyll",
    "cover": "cross-env NODE_ENV=development nyc ava"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssbcs/node-systray.git"
  },
  "keywords": [
    "systray",
    "tray",
    "gui"
  ],
  "author": "cryptix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ssbcs/node-systray/issues"
  },
  "homepage": "https://github.com/ssbc/node-systray#readme",
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/fs-extra": "^4.0.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "cross-env": "^5.0.5",
    "debug": "^3.1.0",
    "del-cli": "^1.1.0",
    "fs-extra": "^4.0.2",
    "mocha": "^5.1.1",
    "nyc": "^11.2.1",
    "ts-node": "^6.0.2",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^6.0.1",
    "typedoc": "^0.8.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "gunzip-maybe": "^1.4.1",
    "adm-zip": "^0.4.11",
    "superagent": "^3.8.3",
    "tar-fs": "^1.16.3"
  }
}
